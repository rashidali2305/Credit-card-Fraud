      "cell_type": "code",
      "source": [
        "%%writefile app.py\n",
        "import streamlit as st\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import joblib\n",
        "import os\n",
        "\n",
        "# --- 1. Load Model and Scaler ---\n",
        "# Define the path to your project folder in Google Drive\n",
        "project_path = \"/content/drive/MyDrive/Colab_Fraud_Detection\"\n",
        "\n",
        "try:\n",
        "    model = joblib.load(os.path.join(project_path, 'xgboost_fraud_model.joblib'))\n",
        "    scaler = joblib.load(os.path.join(project_path, 'robust_scaler.joblib'))\n",
        "except FileNotFoundError:\n",
        "    st.error(\"Model or scaler not found. Please ensure 'xgboost_fraud_model.joblib' and 'robust_scaler.joblib' are in your project directory.\")\n",
        "    st.stop()\n",
        "\n",
        "# --- 2. App UI Configuration ---\n",
        "st.set_page_config(page_title=\"Fraud Detection App\", layout=\"wide\")\n",
        "st.title(\"Credit Card Fraud Detection Dashboard\")\n",
        "st.write(\"\"\"\n",
        "This app uses a trained XGBoost model to predict fraudulent credit card transactions.\n",
        "Use the sidebar to input transaction details and see the model's prediction.\n",
        "\"\"\")\n",
        "\n",
        "# --- 3. Sidebar for User Inputs ---\n",
        "st.sidebar.header(\"Transaction Features\")\n",
        "\n",
        "amount_input = st.sidebar.number_input(\n",
        "    \"Transaction Amount ($)\",\n",
        "    min_value=0.0,\n",
        "    value=1.0,\n",
        "    step=1.0,\n",
        "    key=\"amount_input\"  # Unique key\n",
        ")\n",
        "time_input = st.sidebar.number_input(\n",
        "    \"Time (seconds)\",\n",
        "    min_value=0,\n",
        "    value=406,\n",
        "    key=\"time_input\"  # Unique key\n",
        ")\n",
        "\n",
        "st.sidebar.subheader(\"Key PCA Features (V1-V28)\")\n",
        "st.sidebar.write(\"Adjust these to simulate a fraudulent transaction.\")\n",
        "\n",
        "# Sliders for the most influential features, each with a unique key\n",
        "v4_input = st.sidebar.slider(\"V4\", min_value=-10.0, max_value=15.0, value=4.5, step=0.1, key=\"v4_slider\")\n",
        "v10_input = st.sidebar.slider(\"V10\", min_value=-25.0, max_value=10.0, value=-5.6, step=0.1, key=\"v10_slider\")\n",
        "v11_input = st.sidebar.slider(\"V11\", min_value=-5.0, max_value=15.0, value=2.8, step=0.1, key=\"v11_slider\")\n",
        "v12_input = st.sidebar.slider(\"V12\", min_value=-20.0, max_value=10.0, value=-6.2, step=0.1, key=\"v12_slider\")\n",
        "v14_input = st.sidebar.slider(\"V14\", min_value=-20.0, max_value=10.0, value=-7.0, step=0.1, key=\"v14_slider\")\n",
        "v17_input = st.sidebar.slider(\"V17\", min_value=-25.0, max_value=10.0, value=-6.7, step=0.1, key=\"v17_slider\")\n",
        "\n",
        "# --- 4. Prediction Logic ---\n",
        "if st.sidebar.button(\"Predict\", key=\"predict_button\"):\n",
        "\n",
        "    # a. Scale the user input for Amount and Time\n",
        "    scaled_amount = scaler.transform(np.array([[amount_input]]))[0][0]\n",
        "    scaled_time = scaler.transform(np.array([[time_input]]))[0][0]\n",
        "\n",
        "    # b. Build the full feature vector using all inputs\n",
        "    feature_list = [scaled_amount, scaled_time]\n",
        "    v_features = [0.0] * 28  # Initialize all V features to 0 (their mean)\n",
        "\n",
        "    # c. Update the list with values from the sliders\n",
        "    v_features[3] = v4_input   # V4 is at index 3\n",
        "    v_features[9] = v10_input  # V10 is at index 9\n",
        "    v_features[10] = v11_input # V11 is at index 10\n",
        "    v_features[11] = v12_input # V12 is at index 11\n",
        "    v_features[13] = v14_input # V14 is at index 13\n",
        "    v_features[16] = v17_input # V17 is at index 16\n",
        "\n",
        "    feature_list.extend(v_features)\n",
        "\n",
        "    # d. Convert to a NumPy array and reshape for the model\n",
        "    feature_vector = np.array(feature_list).reshape(1, -1)\n",
        "\n",
        "    # e. Make the prediction\n",
        "    prediction = model.predict(feature_vector)\n",
        "    prediction_proba = model.predict_proba(feature_vector)\n",
        "\n",
        "    # --- 5. Display Results ---\n",
        "    st.subheader(\"Prediction Result\")\n",
        "    if prediction[0] == 1:\n",
        "        st.error(\"This transaction is likely FRAUDULENT!\", icon=\"🚨\")\n",
        "        st.write(f\"**Confidence:** {prediction_proba[0][1]*100:.2f}%\")\n",
        "    else:\n",
        "        st.success(\"This transaction is likely LEGITIMATE.\", icon=\"✅\")\n",
        "        st.write(f\"**Confidence (Legitimate):** {prediction_proba[0][0]*100:.2f}%\")\n",
        "\n",
        "    st.subheader(\"Input Details\")\n",
        "    st.write({\n",
        "        \"Transaction Amount\": f\"${amount_input:.2f}\",\n",
        "        \"Time\": f\"{time_input} seconds\",\n",
        "        \"Scaled Amount\": f\"{scaled_amount:.4f}\",\n",
        "        \"Scaled Time\": f\"{scaled_time:.4f}\"\n",
        "    })\n",
        "\n",
        "    with st.expander(\"Show Full Feature Vector Sent to Model\"):\n",
        "        st.json(dict(zip(['scaled_amount', 'scaled_time'] + [f'V{i+1}' for i in range(28)], feature_list)))"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "gCmKKMkXdwB0",
        "outputId": "170f142b-af4e-40ba-c127-391f6ecb00c7"
