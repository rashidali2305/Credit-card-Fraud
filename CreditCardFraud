      "cell_type": "code",
      "source": [
        "# Cell 4: Train XGBoost and Save Models\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.metrics import classification_report, accuracy_score\n",
        "import xgboost as xgb\n",
        "import joblib\n",
        "\n",
        "# Use the balanced dataset for training\n",
        "X_balanced = balanced_df.drop('Class', axis=1)\n",
        "y_balanced = balanced_df['Class']\n",
        "X_train, X_test, y_train, y_test = train_test_split(X_balanced, y_balanced, test_size=0.2, random_state=42, stratify=y_balanced)\n",
        "\n",
        "# Train XGBoost Model\n",
        "model = xgb.XGBClassifier(objective='binary:logistic', use_label_encoder=False, eval_metric='logloss', random_state=42)\n",
        "model.fit(X_train, y_train)\n",
        "\n",
        "# Evaluate\n",
        "y_pred = model.predict(X_test)\n",
        "print(\"\\n--- XGBoost Classifier Report (on balanced test data) ---\")\n",
        "print(classification_report(y_test, y_pred, target_names=['Legitimate', 'Fraudulent']))\n",
        "\n",
        "# Save the trained model and scaler to your Google Drive\n",
        "joblib.dump(model, 'xgboost_fraud_model.joblib')\n",
        "joblib.dump(rob_scaler, 'robust_scaler.joblib')\n",
        "print(\"\\nModel and Scaler saved successfully to:\", project_path)"
      ],
